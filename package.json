{
  "name": "lineupjs",
  "description": "LineUp is an interactive technique designed to create, visualize and explore rankings of items based on a set of heterogeneous attributes.",
  "version": "1.1.2-SNAPSHOT",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "contributors": [
    {
      "name": "Samuel Gratzl",
      "email": "samuel-gratzl@gmx.at"
    },
    {
      "name": "Marc Streit",
      "email": "marc@streit.com"
    },
    {
      "name": "Holger Stitz",
      "email": "kontakt@holgerstitz.de"
    },
    {
      "name": "Hendrik Strobelt",
      "email": "hstrobelt@seas.harvard.edu"
    }
  ],
  "engines": {
    "npm": ">= 3",
    "node": ">= 6",
    "iojs": ">= 3"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Caleydo/lineupjs/issues"
  },
  "homepage": "https://github.com/Caleydo/lineupjs",
  "main": "build/LineUpJS.js",
  "files": [
    "build",
    "!build/docs",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.scss"
  ],
  "scripts": {
    "compile": "tsc",
    "lint": "tslint --project tsconfig.json --type-check -c tslint.json && stylelint src/**/*.scss",
    "docs": "typedoc --options typedoc.json src",
    "pretest": "npm run compile",
    "test": "echo 'no tests'",
    "posttest": "npm run lint",
    "prebuild": "npm run test",
    "build": "webpack",
    "watch": "webpack --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/lineupjs.tar.gz --exclude \"docs\" * && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Caleydo/lineupjs.git"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-scss": "^1.4.4",
    "tslint": "^5.0.0",
    "typedoc": "^0.5.10",
    "typescript": "2.2.2",
    "webpack": "^2.3.3",
    "@types/handlebars": "4.0.33"
  },
  "optionalDependencies": {
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.20",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "dependencies": {
    "@types/d3": "^3.5.36",
    "d3": "^3.5.17",
    "font-awesome": "^4.6.3"
  }
}
